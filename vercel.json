{
    "version": 2,
    "builds": [
      {
        "src": "*.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/routes/rutas.py"
      }
    ],
    "functions": {
      "api/*": {
        "runtime": "@vercel/python",
        "environment": {
          "MYSQLCLIENT_CFLAGS": "`./mysql_config --cflags`",
          "MYSQLCLIENT_LDFLAGS": "`./mysql_config --libs`"
        }
      }
    }
  }